[core]

[push]
    default = simple
[alias]
    todos = !git status && echo && git loog
    fuck = !git add -A && git commit --amend --no-edit
    sorry = !git add -A && git rebase --continue
    checokut = checkout
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = white bold
[color "diff"]
    new = green
    old = magenta
